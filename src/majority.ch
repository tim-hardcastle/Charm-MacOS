def

boyerMoore (L) :
    getMajority(L[0], 1, 1)
given :
    getMajority = func(current, counter, i) :
        i == len(L) : current
        else :
            counter == 0 : this(L[i], 1, i + 1)
            else : 
                L[i] == current :  this(current, counter + 1, i + 1)
                else : this(current, counter - 1, i + 1) 

