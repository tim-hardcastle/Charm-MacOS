import

golang "fmt"

def

errorf(format string, a tuple) : golang {
    return fmt.Errorf(format, a...)
}

sprint(a tuple) : golang {
    return fmt.Sprint(a...)
}

sprintf(format string, a tuple) : golang {
    return fmt.Sprintf(format, a...)
}

sprintln(a tuple) : golang {
    return fmt.Sprintln(a...)
}
